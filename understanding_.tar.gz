import shutil
import os

data_path = "Data"

for filename in os.listdir(data_path):
    if filename.endswith(".gz"):
        with open(os.path.join(data_path, filename), 'rb') as f_in:
            with open(os.path.join(data_path, filename[:-3]), 'wb') as f_out:
                shutil.copyfileobj(f_in, f_out)
        os.remove(os.path.join(data_path, filename))  # Remove the .gz file after extraction
import tarfile

# Extract .tar files
for filename in os.listdir(data_path):
    if filename.endswith(".tar"):
        with tarfile.open(os.path.join(data_path, filename), 'r') as archive:
            archive.extractall(path=data_path)
